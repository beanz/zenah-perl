[% filternav(filters) %]
[% pagenav(page) %]
<table class="list-table">
  <tr class="list-header">
  [% FOR column = column_order %]
    <th>[% column %]</th>
  [% END %]
    <th/>
  </tr>
  [% SET count = 0 %]
  [% WHILE (item = items.next) %]
    <tr class="list-row-[% count % 2 %]">
    [% FOR column = column_order %]
      <td>[% item.$column %]</td>
    [% END %]
      <td>
        [% crudbutton("map", "view", item.id) %]
        [% crudbutton("map", "edit", item.id) %]
        <!-- [% crudbutton("map", "destroy", item.id) %] -->
      </td>
    </tr>
    [% count = count + 1 %]    
  [% END %]
</table>
<hr/>
[% crudbutton("map", "add", "") %]
